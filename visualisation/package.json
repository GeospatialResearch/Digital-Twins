{
  "name": "digital-twin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
