  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "core-js": "^3.37.1",
    "geo-visualisation-components": "^0.3.0",
    "vue": "^3.4.29",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.13",
    "@vitejs/plugin-vue": "^5.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "typescript": "~5.4.0",
    "vite": "^5.3.5",
    "vite-plugin-static-copy": "^1.0.6",
    "vue-tsc": "^2.0.29"
